.backimage {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
  }

.page{
  background-color: #e6dbbb;
}

.content-box {
  margin-top:500rpx;
  margin-left: 25rpx;
  background-color:white;
  height: 300rpx;
  width: 700rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  opacity: 0.8;
  border-radius: 30rpx;
}

.content-box-item {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.text01 {
  width: 280px;
  font-size: 16px;
  line-height: 30px;
  justify-content: center;
  align-items: center;
}

.img01 {
  width: 280px;
  height: 280px;
  margin-top: 6px;
}

.text02 {
  font-size: 12px;
  color: #666;
  line-height: 30px;
}

.btn-box {
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  border-top: 1rpx solid #d8d8d8;
  z-index: 999;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
}

.btn-copy {
  height: 44px;
  background: #455ac0;
  width: 686rpx;
  margin-top: 10px;
  border-radius: 4rpx;
  color: #fff;
  font-size: 16px;
  line-height: 44px;
}

.btn8{
  width: 200rpx;
  height: 200rpx;
  margin-top: 20rpx;
  background-color:transparent;
  color: #999999;
  border-radius: 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  z-index: 10000;
}
