.backimage {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
  }

.page {
    padding: 20rpx;
  }
 
  .text{
    position: fixed;
    bottom: 0rpx;
    width: 650rpx;    /*弹窗布局宽*/
    height: 200rpx;   /*弹窗布局高，与下面弹出距离transform有关*/
    margin-left: 28rpx;
    margin-bottom: 70rpx;
    background: #f8f8f8;
    z-index: 12;
    border-radius: 20rpx;
    box-shadow: 0px 3px 3px 2px gainsboro;
    padding: 25rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    opacity: 0.9;
  }

  .nextSubmit{
      position: fixed;
      bottom: 0rpx;
      width: 650rpx;    /*弹窗布局宽*/
      height: 200rpx;   /*弹窗布局高，与下面弹出距离transform有关*/
      margin-left: 28rpx;
      background: transparent;
      z-index: 12;
      border-radius: 20rpx;
      padding: 25rpx;
      display: flex;
      align-items: center;
      justify-content: center;
      opacity: 0.9;
  }


