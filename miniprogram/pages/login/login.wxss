/* pages/login.wxss */
.content{
    margin-top: 40px;
  }
  .account{
     display: flex;
     flex-direction: row;
     padding-left: 20px;
     padding-top: 20px;
     padding-bottom: 10px;
     width: 90%;
     position:fixed;
  }
  .title{
     margin-right: 30px;
     font-weight: bold;
     position: relative;
  }
  .hr{
    border: 1px solid #cccccc;
    opacity: 0.2;
    width: 90%;
    margin: 0 auto;
    background-color: red;
  }
  .btn{
    width: 90%;
    margin-top:40px;
    color: #999999;
  }
  
  .titleImage{
    margin-top: 25rpx;
    display:grid;
    justify-content: center;         
    align-items:center; 
    position:relative;              
   }
 
.background {
    width: 747rpx;
    height: 643.8rpx;
    left: 5rpx;
    position:fixed; 
    background-size:100% 100%;
    z-index: 10;
    position: absolute;
    justify-content: center;
}
