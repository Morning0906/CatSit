.input-wrap {
  display: flex;
  flex-direction: row;
  position:relative;
  justify-content: center;
  text-align: center;
  border: 1rpx solid black;
  border-radius: 19px;
}

.container1 {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
